{
  "$defs": {
    "TemplateConfig": {
      "properties": {
        "file_id": {
          "description": "Template image file name without extension",
          "title": "File Id",
          "type": "string"
        },
        "coords": {
          "$ref": "#/$defs/WplacePixelCoords",
          "description": "Top-left pixel coordinates of the template on the canvas"
        }
      },
      "required": [
        "file_id",
        "coords"
      ],
      "title": "TemplateConfig",
      "type": "object"
    },
    "UserConfig": {
      "properties": {
        "identifier": {
          "description": "User identifier, for logging purposes",
          "title": "Identifier",
          "type": "string"
        },
        "credentials": {
          "$ref": "#/$defs/WplaceCredentials",
          "description": "Wplace authentication credentials"
        },
        "template": {
          "$ref": "#/$defs/TemplateConfig",
          "description": "Template configuration"
        }
      },
      "required": [
        "identifier",
        "credentials",
        "template"
      ],
      "title": "UserConfig",
      "type": "object"
    },
    "WplaceCredentials": {
      "properties": {
        "token": {
          "description": "Playwright cookie 'j' value",
          "title": "Token",
          "type": "string"
        },
        "cf_clearance": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Playwright cookie 'cf_clearance' value, could be null if not needed",
          "title": "Cf Clearance"
        }
      },
      "required": [
        "token"
      ],
      "title": "WplaceCredentials",
      "type": "object"
    },
    "WplacePixelCoords": {
      "properties": {
        "tlx": {
          "title": "Tlx",
          "type": "integer"
        },
        "tly": {
          "title": "Tly",
          "type": "integer"
        },
        "pxx": {
          "title": "Pxx",
          "type": "integer"
        },
        "pxy": {
          "title": "Pxy",
          "type": "integer"
        }
      },
      "required": [
        "tlx",
        "tly",
        "pxx",
        "pxy"
      ],
      "title": "WplacePixelCoords",
      "type": "object"
    }
  },
  "properties": {
    "users": {
      "description": "List of user configurations",
      "items": {
        "$ref": "#/$defs/UserConfig"
      },
      "title": "Users",
      "type": "array"
    },
    "browser": {
      "description": "Playwright browser type to use",
      "enum": [
        "chromium",
        "firefox",
        "webkit",
        "chrome",
        "msedge"
      ],
      "title": "Browser",
      "type": "string"
    }
  },
  "required": [
    "users",
    "browser"
  ],
  "title": "Config",
  "type": "object"
}